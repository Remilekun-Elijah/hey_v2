<div id="carouselExampleIndicators" class="carousel slide mt-0" data-bs-ride="carousel">
 <div class="carousel-indicators">

  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
  <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
 </div>
 <div class="carousel-inner mt-0">
  <%- include('../templates/nav.ejs') %>

   <div class="carousel-item ci-one active">

    <div class="d-flex align-items-center justify-content-center h-100">
     <div class="d-non d-md-block text-white container  mt-4" style="width: 60%;">
      <p class="text-center caption mb-0 pb-0 ">The future of freight</p>
      <p class="text-center caption mt-0 pt-0"> at your finger tip.</p>
      <div class="d-flex justify-content-between my-5">
       <p>Air cargo</p>
       <p>Ocean cargo</p>
       <p>Land cargo</p>
      </div>
      <center><button class="btn btn-blue px-4">Ship with us</button></center>
     </div>
    </div>
   </div>

   <div class="carousel-item ci-two">
    <div class="d-flex align-items-center justify-content-center h-100" style="background-color: rgba(0, 0, 0, .7);">
     <div class="w-50 caption-wrapper">
      <div class="text-white">
       <p class="text-left caption mb-0 pb-0 ">Shop from any E-Commerce</p>
       <p class="text-left caption my-0 py-0"> stores in USA, UK and China</p>
       <p class="text-left caption mt-0 pt-0"> and Airfreight to Nigeria.</p>

      </div>
      <img src="/images/amazon.png" height="35px" alt="">
      <img src="/images/ebay.png" alt="">
      <img src="/images/zara.png" alt="">
      <img src="/images/newLogo.png" alt="">
      <img src="/images/wallmart.png" height="40px" width="100px" alt="">

     </div>
     <div class="d-flex justify-content-between my-5" style="height: 90%; align-self: baseline;">
      <img src="/images/deliveryMan.svg" alt="" class="">
     </div>
    </div>

    <div class="button-grou w-100 d-flex justify-content-center" style="position: absolute; bottom: 5em;">
     <button class="btn btn-blue px-5 me-4">Create account</button>
     <button class="btn btn-blue px-5">Speak with us</button>

    </div>
   </div>

   <div class="carousel-item ci-three">

    <div class="d-flex align-items-center pt-5 justify-content-center h-100" style="background-color: rgba(0, 0, 0, .7);">
     <div class="d-non d-md-block text-white container  mt-4" style="width: 70%;">
      <p class="text-center caption mb-0 pb-0 ">Can't find it locally ? Buy from USA, UK and</p>
      <p class="text-center caption my-0 pt-0"> we will deliver to you in Nigeria</p>
      <p class="text-center caption mt-0 pt-0"> starting @ $3.99 per pounds in 3/7days.</p>
      <div class="d-flex justify-content-between my-5">
       <p>Safe</p>
       <p>Swift</p>
       <p>Smiles</p>
      </div>
      <center><button class="btn btn-blue px-4">Ship with us</button></center>
     </div>
    </div>
   </div>

 </div>
